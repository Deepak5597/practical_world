<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css" integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">
    <link rel="stylesheet" href="../css/main.css">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@100;300;400;500;700&display=swap" rel="stylesheet">
    <title>Practical World</title>
  </head>
  <body>
    
    <div class="main-wrapper">
      <div class="container-fluid top-nav">
        <nav class="row">
            <div class="col">
                <img class="logo" src="../images/pw-logo-final.png">
            </div>
            <div class="col">
                <ul class="social-links">
                  <li class=""><a href="javscript:void(0)"><i class="fa fa-github"></i></a></li>
                </ul>
            </div>
        </nav>
      </div>
        <div class="main-content-wrapper">
          <div class="row no-gutters main-quote_row">
            <div class="col">
              <div class="main-quote-block">
                <blockquote class="blockquote text-center">
                  <p class="mb-0 quote-text">Learning never exhausts the mind.</p>
                  <footer class="blockquote-footer"><cite title="Source Title">Leonardo da Vinchi</cite></footer>
                </blockquote>
                <button class="btn text-white bg-dark dropdown-toggle" data-toggle="dropdown"> let's learn</button>
                <div class="dropdown-menu">
                  <% for(var i =0;i<postConfig.length;i++) {%>
                  <a class="dropdown-item" href="posts/<%=postConfig[i].mainHeadingKey%>/<%=postConfig[i].primarySubKey%>"><%=postConfig[i].mainHeadingLabel%></a>
                  <%}%>
                </div>
              </div>
            </div> 
          </div>
        </div>
        <%- include(`./partials/footer.ejs`) %>
    </div> 

    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.min.js" integrity="sha384-OgVRvuATP1z7JjHLkuOU7Xw704+h835Lr+6QL9UvYjZE3Ipu6Tp75j7Bh/kR0JKI" crossorigin="anonymous"></script>
  </body>
</html>